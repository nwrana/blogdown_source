#cycle period
Tphys <- 23
Temo <- 28
Tment <- 33
#####NATHAN#######
Nphys.plot <- NULL
for(i in 0:(length(n.dates)-1)) {
temp <- sin(2*pi*i*1/Tphys)
Nphys.plot <- rbind(Nphys.plot,temp)
}
Nemo.plot <- NULL
for(i in 0:(length(n.dates)-1)) {
temp <- sin(2*pi*i*1/Temo)
Nemo.plot <- rbind(Nemo.plot,temp)
}
Nment.plot <- NULL
for(i in 0:(length(n.dates)-1)) {
temp <- sin(2*pi*i*1/Tment)
Nment.plot <- rbind(Nment.plot,temp)
}
